{
  "compilerOptions": {
    "sourceMap": true,
    "lib": [
      "es5",
      "es2015.promise",
      "esnext.asynciterable" /* error TS2304: Cannot find name 'AsyncIterator' */
    ],
    "target": "es5",
    "moduleResolution": "node",
    "rootDir": "./",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": ".build",
    "types": ["reflect-metadata"]
  },
  "exclude": [ "node_modules", "build" ],
  "types": [ "typePatches" ]
}
